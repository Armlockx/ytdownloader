{
  "buildCommand": "npm run build",
  "outputDirectory": "client/build",
  "installCommand": "npm install && cd client && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "functions": {
    "api/video-info.js": {
      "runtime": "nodejs18.x",
      "memory": 1024,
      "maxDuration": 60
    }
  }
}

